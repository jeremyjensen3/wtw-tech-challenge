# Default Catch-All
server {
       listen 3200;
       listen [::]:3200;
       
       error_page 404 /custom_404.html;

       # Everything is a 404
       location / {
        return 404; #return the code 404
       }

       location = /custom_404.html {
                root /var/www/html;
                internal;
        }

}